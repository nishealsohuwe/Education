using System;

namespace Task_1
{
    class Program
    {
        static void Main(string[] args)
        {
            string fisrtName = "Екимовских";
            string secondName = "Александр";

            Console.WriteLine("Имя: " + fisrtName);
            Console.WriteLine("Фамилия: " + secondName);

            string exchangeBuffer = fisrtName;
            fisrtName = secondName;
            secondName = exchangeBuffer;

            Console.WriteLine("Имя: " + fisrtName);
            Console.WriteLine("Фамилия: " + secondName);
        }
    }
}
